{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true,
        "es6": true
    },
    "rules": {
        "indent": [2, 2, { "SwitchCase": 1, "VariableDeclarator": 1}],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
